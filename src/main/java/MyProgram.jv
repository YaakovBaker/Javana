Javana MyProgram:
    decl myVar: int
    decl myBool: bool
    record MyRecord {
        field1: int
        field2: bool
    }

    func myFunction() -> int {
        var x = 5
        var y = 10
        println(x + y)
        return x
    }

    @main() {
        var arr = @int[5]
        var rec = @MyRecord{field1=10, field2=true}
        myFunction()
        println("Program executed successfully.")
    }
